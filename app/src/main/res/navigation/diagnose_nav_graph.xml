<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/diagnose_nav_graph"
    app:startDestination="@id/introduceFragment">

    <fragment
        android:id="@+id/introduceFragment"
        tools:layout="@layout/fragment_introduce_diagnose"
        android:name="com.example.brainwest_android.ui.diagnose.introduce.IntroduceDiagnoseFragment">
        <action
            android:id="@+id/action_introduceFragment_to_questionFragment"
            app:destination="@id/questionFragment"
            app:enterAnim="@anim/zoom_fade_in"
            app:exitAnim="@anim/zoom_fade_out"
            app:popEnterAnim="@anim/zoom_fade_in"
            app:popExitAnim="@anim/zoom_fade_out"/>
    </fragment>

    <fragment
        android:id="@+id/questionFragment"
        tools:layout="@layout/fragment_question_diagnose"
        android:name="com.example.brainwest_android.ui.diagnose.question.QuestionDiagnoseFragment">
        <action
            android:id="@+id/action_questionFragment_to_resultFragment"
            app:destination="@id/resultFragment"
            app:enterAnim="@anim/zoom_fade_in"
            app:exitAnim="@anim/zoom_fade_out"
            app:popEnterAnim="@anim/zoom_fade_in"
            app:popExitAnim="@anim/zoom_fade_out"/>
    </fragment>

    <fragment
        android:id="@+id/resultFragment"
        tools:layout="@layout/fragment_result_diagnose"
        android:name="com.example.brainwest_android.ui.diagnose.result.ResultDiagnoseFragment"/>

</navigation>